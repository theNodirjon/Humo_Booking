                            app:layout_constraintEnd_toEndOf="@id/xitoy_img"
                            app:layout_constraintTop_toBottomOf="@id/xitoy_img"
                            tools:ignore="MissingConstraints" />

                        <com.google.android.material.imageview.ShapeableImageView
                            android:id="@+id/misr_img"
                            android:layout_width="200dp"
                            android:layout_height="150dp"
                            app:shapeAppearanceOverlay="@style/imageCorner"
                            android:background="@drawable/egypt_img"
                            app:layout_constraintTop_toBottomOf="@id/visa_info"
                            app:layout_constraintStart_toEndOf="@id/xitoy_img"
                            app:layout_constraintEnd_toEndOf="parent"
